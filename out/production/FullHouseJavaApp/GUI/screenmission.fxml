<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.missionController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="500.0">
         <children>
            <Label layoutX="14.0" layoutY="32.0" text="Mission list:" />
            <TextField fx:id="TFsearchMission" layoutX="329.0" layoutY="307.0" />
            <Button fx:id="BTNsearchMission" layoutX="323.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#SearchMission" text="Search" />
            <Label layoutX="325.0" layoutY="281.0" text="Search missionMark:" />
            <Button fx:id="BTNreateMission" layoutX="323.0" layoutY="153.0" mnemonicParsing="false" onMouseClicked="#newmission" text="Create" />
            <TextField fx:id="TFlocationXMission" layoutX="325.0" layoutY="56.0" />
            <TextField fx:id="TFlocationYMission" layoutX="325.0" layoutY="114.0" />
            <Label layoutX="327.0" layoutY="32.0" text="Location X:" />
            <Label layoutX="329.0" layoutY="90.0" text="location Y:" />
            <ListView fx:id="missionlist" layoutX="14.0" layoutY="53.0" prefHeight="326.0" prefWidth="274.0" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
