<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.createMissionController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="558.0">
         <children>
            <Button fx:id="BTNreateMission" layoutX="253.0" layoutY="326.0" mnemonicParsing="false" onMouseClicked="#createMission" prefHeight="57.0" prefWidth="149.0" text="maak aan" />
            <TextField fx:id="TFlocationXMission" layoutX="37.0" layoutY="235.0" promptText="x coordinaat locatie" />
            <TextField fx:id="TFlocationYMission" layoutX="37.0" layoutY="295.0" promptText="y coordinaat locatie" />
            <Label layoutX="39.0" layoutY="214.0" text="Locatie X:" />
            <Label layoutX="41.0" layoutY="274.0" text="locatie Y:" />
            <TextField fx:id="TFnameMission" layoutX="35.0" layoutY="49.0" prefHeight="31.0" prefWidth="292.0" promptText="naam van de missie" />
            <Label layoutX="37.0" layoutY="28.0" text="Naam" />
            <TextArea fx:id="TAdescriptionMission" layoutX="35.0" layoutY="120.0" prefHeight="79.0" prefWidth="477.0" promptText="omschrijving van de missie" wrapText="true" />
            <Label layoutX="37.0" layoutY="93.0" prefHeight="21.0" prefWidth="102.0" text="Omschrijving" />
            <TextField fx:id="TFestimatedTime" layoutX="37.0" layoutY="354.0" promptText="Tijdsduur van de missie" />
            <Label layoutX="39.0" layoutY="333.0" prefHeight="21.0" prefWidth="95.0" text="verwachte tijd" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
